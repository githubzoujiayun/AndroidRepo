<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/filter_green_008d14"/>
            <stroke
                android:width="1dp"
                android:color="@color/filter_green_008d14"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/filter_green_008d14"/>
            <stroke
                android:width="1dp"
                android:color="@color/filter_green_008d14"/>
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <solid android:color="@color/filter_green_77008d14"/>
            <stroke
                android:width="1dp"
                android:color="@color/filter_green_008d14"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#FFFFFF"/>
            <stroke
                android:width="1dp"
                android:color="@color/filter_green_008d14"/>
        </shape>
    </item>
</selector>


