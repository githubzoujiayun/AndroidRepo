<?xml version="1.0" encoding="utf-8"?>

<jobcase 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="jobtest.xsd"
    enabled="false">
    <click>
	    <text>"校园招聘"</text>
        <text>"科系搜索"</text>
        <text>"医学"</text>
    </click>
    <wait
        type="wait_until_gone"
        timeout="20000"
        >
        <text
            textMatches="数据加载中。*"
            />
    </wait>
    <click>
        <text resId="com.job.android:id/rightImageButton"/>
        <text>"医学类"</text>
        <text>"培训"</text>
    </click>
    <wait
        type="wait_until_gone"
        timeout="20000"
        >
       <text
            textMatches="数据加载中。*"
            />
    </wait>
    <click>
        <indexView
			rootClass="android.widget.ListView"
			rootIndex="0"
			indexs="2:0"
            >
        </indexView>
    </click>
    <record condition="view_exist">
        <text
            scrollable="VERTICAL"
            text="举报职位"
		/>
    </record>
</jobcase>