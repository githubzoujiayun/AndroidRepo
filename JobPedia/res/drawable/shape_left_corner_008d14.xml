<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/green_2de3a3"/>
            <corners
                android:bottomLeftRadius="@dimen/commen_radius"
                android:topLeftRadius="@dimen/commen_radius"/>
            <stroke
                android:width="1dp"
                android:color="@color/green_2de3a3"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/green_2de3a3"/>
            <corners
                android:bottomLeftRadius="@dimen/commen_radius"
                android:topLeftRadius="@dimen/commen_radius"/>
            <stroke
                android:width="1dp"
                android:color="@color/green_2de3a3"/>
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <solid android:color="@color/green_77008d14"/>
            <corners
                android:bottomLeftRadius="@dimen/commen_radius"
                android:topLeftRadius="@dimen/commen_radius"/>
            <stroke
                android:width="1dp"
                android:color="@color/green_2de3a3"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#FFFFFF"/>
            <corners
                android:bottomLeftRadius="@dimen/commen_radius"
                android:topLeftRadius="@dimen/commen_radius"/>
            <stroke
                android:width="1dp"
                android:color="@color/green_2de3a3"/>
        </shape>
    </item>
</selector>